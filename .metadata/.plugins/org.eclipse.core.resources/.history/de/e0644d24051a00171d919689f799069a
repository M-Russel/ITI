#ifndef SPI_PRIVATE_H_
#define SPI_PRIVATE_H_


/*SPI Data Register :- */
#define SPDR (*((volatile pu8)(0x2F)))
/*SPI Status Register */
#define SPSR (*((volatile pu8)(0x2E)))
/*SPI Control Register */
#define SPCR (*((volatile pu8)(0x2D)))

#define SPIE     7
#define SPE      6
#define DORD  5
#define MSTR   4
#define CPOL   3
#define CPHA   2
#define SPR1    1
#define SPR0    0
#endif /* SPI_PRIVATE_H_ */
