#ifndef INT0_CONFIG_H_
#define INT0_CONFIG_H_


#define ISC11_STATUS
#define ISC10_STATUS
#define ISC01_STATUS
#define ISC00_STATUS
//MCU Control and Status Register
#define ISC2_STATUS

//General Interrupt Control Register
#define INT1_STATUS
#define INT0_STATUS
#define INT2_STATUS

//General Interrupt Flag Register
#define INTF1_STATUS
#define INTF0_STATUS
#define INTF2_STATUS


#endif /* INT0_CONFIG_H_ */
