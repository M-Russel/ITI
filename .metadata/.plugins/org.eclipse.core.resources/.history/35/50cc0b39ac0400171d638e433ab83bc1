#ifndef ADC_CONFIG_H_
#define ADC_CONFIG_H_


/*
 * ADC Multiplexer Selection Register
 * */

#define REFS1_STATUS DISABLED
#define REFS0_STATUS ENABLED
#define ADLAR_STATUS DISABLED
#define MUX4_STATUS  DISABLED
#define MUX3_STATUS  DISABLED
#define MUX2_STATUS  DISABLED
#define MUX1_STATUS  DISABLED
#define MUX0_STATUS  DISABLED

#endif /* ADC_CONFIG_H_ */
