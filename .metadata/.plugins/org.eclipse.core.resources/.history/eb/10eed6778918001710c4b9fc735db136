void main(void)
{
	DDRB=0xff;
	DDRD=0xff;
	u16 x=0;


LCD_init();
/*for(x=0;x<504;x++)
{
	arr[x]=0xff;
}*/


while(1)
{
	LCD_update();
}



}
void LCD_init(void)
{
	DIO_u8SetPortDirection(&DDRB,DIO_PORT_OUTPUT);
	DIO_u8SetPortDirection(&DDRD,DIO_PORT_OUTPUT);
	SPCR=0B01010001;
	LCD|=(1<<CE);
	LCD_write_cmd(0b00100001); //extended command
	LCD_write_cmd(0xbf); //set Vop ( Contrast )
	LCD_write_cmd(0b00010011); //bias voltage
	LCD_write_cmd(0b00000110); //temp
	LCD_write_cmd(0b00100000); //basic command
	LCD_write_cmd(0b00001100); //D and E
	/*DDRD=0xff;
	DDRB=0xff;
	SPCR =0b01010001;
	SPSR&=~(0x01);
	PORTD|=0x02; //CE=1
	LCD_write_cmd(0b00100001); //extended instruction set
	LCD_write_cmd(0xBF); // set Vop
	LCD_write_cmd(0b00010011); //bias voltage = 4
	LCD_write_cmd(0b00000110); 		 //temperature
	LCD_write_cmd(0b00100000);   //basic instruction set
	LCD_write_cmd(0b00001100);   //normal mode

*/



}

void LCD_write_cmd(u8 cmd)
{
	LCD&=~(1<<DC);
	LCD&=~(1<<CE);
	SPDR=cmd;
	while(!(SPSR&(1<<SPIF)));
	LCD|=(1<<CE);




}
void LCD_write_data(u8 data)
{
	LCD|=(1<<DC);
	LCD&=~(1<<CE);
	SPDR=data;
	while(!(SPSR&(1<<SPIF)));
	LCD|=(1<<CE);




}
void LCD_update(void)
{
	u16 i=0;
	u16 j=0;
	//x=0;
	//y=0;
	LCD_write_cmd(0b01000000);
	LCD_write_cmd(0b10000000);
	for(i=0;i<8;i++)
	{
	for(j=0;j<504;j++)
	{

		LCD_write_data(pgm_read_byte(&arr[i][j]));
	}
	_delay_ms(200);
	}



}
