#include "DIO_Interface.h"
#define CE 0
#define DC 1
#define LCD PORTD

#define SPCR (Addressing *)(0x2d)





void LCD_write_cmd(u8 cmd);
void LCD_write_data(u8 data);
void LCD_init(void);
void LCD_update(void);

typedef union
{

struct {
	u8 Bit1:1;
	u8 Bit2:1;
	u8 Bit3:1;
	u8 Bit4:1;
	u8 Bit5:1;
	u8 Bit6:1;
	u8 Bit7:1;
	u8 Bit8:1;
}BitAddres;
u8 Byte_Address;

}Addressing;
/*
 * spcr
 * {spe(spi enable),1
 * dord(data order),0
 * mstr(master or slave),1
 * cpol(clock polarity),0
 * cpha(clock phase ),0
 * spie(spi interrupt enable)
 * spr1(clock) 0 (/4)
 * spr0(clock) 0 (/4)}
 *
 * spsr
 * {
 * spi2x(douple speed),0
 * spif(interrupt flag),
 * wcol(write collision flag)}
 *
 * spdr (data register)
 *
 */
