#ifndef UART_CONFIG_H_
#define UART_CONFIG_H_



#define TXC_STATUS DISABLED
#define U2X_STATUS DISABLED
#define MPCM_STATUS DISABLED



#define RXCIE_STATUS DISABLED
#define TXCIE_STATUS DISABLED
#define UDRIE_STATUS DISABLED
#define RXEN_STATUS DISABLED
#define TXEN_STATUS DISABLED
#define UCSZ2_STATUS DISABLED
#define TXB_STATUS DISABLED


#define URSEL 7
#define UMSEL 6
#define UPM1 5
#define UPM0 4
#define USBS 3
#define UCSZ1 2
#define UCSZ0 1
#define UCPOL 0



#endif /* UART_CONFIG_H_ */
