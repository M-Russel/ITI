#ifndef ADC_CONFIG_H_
#define ADC_CONFIG_H_


/*
 * ADC Multiplexer Selection Register
 * */

#define REFS1_STATUS DISABLED
#define REFS0_STATUS ENABLED
#define ADLAR_STATUS
#define MUX4_STATUS
#define MUX3_STATUS
#define MUX2_STATUS
#define MUX1_STATUS
#define MUX0_STATUS

#endif /* ADC_CONFIG_H_ */
