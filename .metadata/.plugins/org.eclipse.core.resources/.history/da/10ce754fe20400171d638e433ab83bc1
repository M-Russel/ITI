#include "std_types.h"
#include "utils.h"
#include "DIO_Interface.h"
#include "DIO_Private.h"
#include "ADC_interface.h"
#include "delay_ms.h"
#define LED_ZERO 1
#define LED_ONE 3

void main (void)
{
	u8 MAIN_u8LocalADC_Analog_Value;
DIO_voidInitialize();
ADC_u8Init();

while (TRUE)
{
ADC_u8ReadADC_Value(ZERO,&MAIN_u8LocalADC_Analog_Value);
if (MAIN_u8LocalADC_Analog_Value > 2)
{
DIO_u8SetPinValue(PIN_16,DIO_LOW);
}
else
{
DIO_u8SetPinValue(PIN_16,DIO_HIGH);
}
}



}
